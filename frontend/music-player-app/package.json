    "glob": "^10.4.5",
    "globals": "^16.0.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.16.0",
    "postcss": "^8.5.3",
    "prettier": "3.5.3",
    "sharp": "^0.34.2",
    "tailwindcss": "^4.1.7",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.7.6"
