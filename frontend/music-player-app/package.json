    "@tsparticles/engine": "^3.8.1",
    "@tsparticles/vue3": "^3.0.1",
    "axios": "^1.9.0",
    "element-plus": "^2.9.11",
    "pinia": "^3.0.1",
    "tsparticles": "^3.8.1",
    "tsparticles-engine": "^2.12.0",
    "vue": "^3.5.15",
    "vue-confetti-explosion": "^1.0.2",
    "vue-router": "^4.5.0",
